page{
  background-image: url("https://s3.bmp.ovh/imgs/2022/05/17/6eb8497f9a1c73d9.png");
  background-repeat:repeat;
  background-size:cover;
}

.topview{
  display: flex;
  align-items: center;
  justify-content: center;   
  width: 100%;
  height: 150px;
}
.topview_img{
  height: 150px;
}

.rankingview{
  display: flex;
  flex-direction:column;
  justify-content: space-between;
}
.user{
  margin-left: 12rpx;
  margin-top: 20rpx;
  height: 150rpx;
  width: 700rpx;
  border: 12rpx solid #8eacd4a2;
  display: flex;
  justify-content: start;
  border-radius: 30rpx;
  background-color: rgba(255, 255, 255, 0.438);
}
.ranking_no{
  display: flex;
  width: 100rpx;
  align-items: center;
  font-size: 80rpx;
  font-weight:900;
  margin-left: 20rpx;
}
.imgview{
  margin-left: 20rpx;
  height: 150rpx;
  width: 120rpx;
  display: flex;
  align-items: center;
}
.user_img{
  display: block;
  height: 120rpx;
  width: 120rpx;
}
.user_name{
  display: flex;
  align-items: center;
  font-size: 40rpx;
  font-weight:600;
  margin-left: 20rpx;
  width: 100rpx;
}
.score{
  display: flex;
  /* position: absolute; */
  align-items: center;
  font-size: 40rpx;
  font-weight:600;
  margin-left: 110rpx;
  width: 140rpx;
}
.fen{
  display: flex;
  align-items: center;
  font-size: 40rpx;
  font-weight:600;
}